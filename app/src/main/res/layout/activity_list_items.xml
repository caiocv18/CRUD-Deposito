<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/activity_list_itens_fab_new_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="26dp"
        android:clickable="true"
        android:src="@drawable/ic_action_new_item"
        android:focusable="true"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/activity_list_itens_fab_share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="86dp"
        android:clickable="true"
        android:src="@drawable/ic_action_share"
        android:focusable="true"/>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/activity_list_itens_fab_video"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="146dp"
        android:clickable="true"
        android:src="@drawable/ic_action_video"
        android:focusable="true"/>

    <ListView
        android:id="@+id/activity_list_items_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </ListView>

    <TextView
        android:id="@+id/tv_resultado"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></TextView>

</RelativeLayout>